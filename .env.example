# Example .env file for Safelift AI project
# Copy this to .env and fill in your actual values
# DO NOT commit .env to version control!

# Django secret key (generate a secure one)
SECRET_KEY=your-secret-key-here

# Debug mode (True/False)
DEBUG=True

# Allowed hosts (comma-separated)
ALLOWED_HOSTS=localhost,127.0.0.1

# CORS allowed origins (comma-separated)
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# JWT signing key (generate a separate secure key)
JWT_SIGNING_KEY=your-jwt-signing-key-here

# Database (optional, override if not using SQLite)
# DATABASE_URL=sqlite:///db.sqlite3
