[run]
source = .
omit = 
    */migrations/*
    */tests/*
    */venv/*
    */env/*
    */__pycache__/*
    manage.py
    safelift/asgi.py
    safelift/wsgi.py
    */site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if settings.DEBUG
    pass
    
precision = 2

[html]
directory = htmlcov

